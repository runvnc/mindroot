# MindRoot TUI Configuration Example
# Copy this file to .env and fill in your values

# Required: Your MindRoot API key
# Get this from the web interface: Settings -> API Keys
MINDROOT_API_KEY=your_api_key_here

# Optional: MindRoot server URL
# Default: http://localhost:8010
MINDROOT_BASE_URL=http://localhost:8010

# Optional: Default agent to use
# Default: Assistant
MINDROOT_DEFAULT_AGENT=Assistant

# Optional: Enable debug mode
# Default: false
MR_DEBUG=false
